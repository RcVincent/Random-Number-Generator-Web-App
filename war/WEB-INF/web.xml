<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  	<servlet>
  	<servlet-name>IndexServlet</servlet-name>
  	<servlet-class></servlet-class>
  	</servlet>
  	
  	
  	<servlet>
  	<servlet-name> RNGServlet </servlet-name>
  	<servlet-class> vincent.ryan.c.RNG.servlet.RNGServlet</servlet-class>
  	</servlet>
  	
  <!-- AJAX servlets -->
  <servlet> 
  	<servlet-name> RNGAjaxServlet </servlet-name>
  	<servlet-class>vincent.ryan.c.RNG.servlet.ajax.RNGAjaxServlet</servlet-class>
  </servlet>
  
  <!-- Servlet Mappings -->
  <servlet-mapping> 
  	<servlet-name> IndexServlet </servlet-name>
  	<url-pattern> index </url-pattern>
  </servlet-mapping>
 
  <servlet-mapping>
  	<servlet-name> RNGServlet</servlet-name>
  	<url-pattern>RNG </url-pattern>
  </servlet-mapping>
  
  
    <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>